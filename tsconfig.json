{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": ".",
    "module": "commonjs",
    "lib": ["dom"],
    "allowJs": true,
    "jsx": "react-jsx",
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "paths": {
      "_/*": ["src/*"],
      "_public/*": ["public/*"],
      "_models/*": ["src/models/*"],
      "_electron/*": ["electron/*"],
      "_services/*": ["src/Services/*"],
      "nodemailer_react": ["../src"]
    }
  }
}
